# Gemini API Key (for Gemini translation engine)
VITE_GEMINI_API_KEY=your_gemini_api_key_here

# OpenRouter API Key (optional, for alternative AI models)
VITE_OPENROUTER_API_KEY=your_openrouter_api_key_here

# Argos Translate local server URL
# Default: http://127.0.0.1:5100/translate
# Start the server with: cd argos-server && python server.py
VITE_ARGOS_SERVER_URL=http://127.0.0.1:5100/translate
